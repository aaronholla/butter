{
  "name": "butter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-server-micro": "^2.9.0",
    "graphql": "^14.5.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "prisma2": "^2.0.0-preview-9.1"
  },
  "scripts": {
    "start": "npm-run-all -l -p dev-now dev-prisma",
    "dev-now": "now dev",
    "dev-prisma": "prisma2 dev",
